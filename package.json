{
  "name": "pushin",
  "description": "A javascript plugin to attach a 'dolly' or 'push-in' effect to a div element when user scrolls.",
  "version": "4.0.1",
  "sideEffects": true,
  "module": "dist/esm/pushin.js",
  "main": "dist/umd/pushin.js",
  "typings": "dist/esm/index.d.ts",
  "unpkg": "dist/umd/pushin.min.js",
  "exports": {
    "./pushin.css": {
      "style": "./dist/pushin.css"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "husky": "husky",
    "lint-staged": "lint-staged",
    "lint": "eslint --cache src/*.ts",
    "build": "rollup -c",
    "docs": "webpack --mode=production",
    "start": "webpack serve --mode=development",
    "mocha": "cross-env TS_NODE_PROJECT=\"tsconfig.spec.json\" mocha -r ts-node/register test/*.spec.ts",
    "test": "npm run mocha -- --watch",
    "coverage": "nyc --reporter=html --reporter=text npm run mocha && open coverage/index.html -a 'Google Chrome'",
    "test:once": "npm run mocha",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nateplusplus/pushin.git"
  },
  "keywords": [
    "parallax",
    "zoom",
    "push-in",
    "movement",
    "motion",
    "effect"
  ],
  "author": "Nathan Blair <nate@natehub.net> (https://natehub.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nateplusplus/pushin/issues"
  },
  "homepage": "http://nateplusplus.github.io/pushin/",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/chai": "^4.3.0",
    "@types/chai-spies": "^1.0.3",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "clean-css": "^5.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.5.2",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "prettier": "^2.6.1",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "rollup": "2.70.1",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.8",
    "ts-mocha": "^9.0.2",
    "typescript": "^4.6.3",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "prettier": {
    "endOfLine": "lf",
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write"
    ]
  }
}
